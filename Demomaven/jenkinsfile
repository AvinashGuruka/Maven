pipeline {
   agent any
   stages {
     stage('git clone'){
       steps{
         git 'https://github.com/AvinashGuruka/Maven.git'
       }
     }
     stage('build'){
       steps{
         bat 'clean install'
       }
     }
     stage('test'){
       steps{
         echo "deploy is successful"        
       }
     }
     stage('deploy'){
       steps{
         echo "deploy is successful"
       }
     }
   }
}